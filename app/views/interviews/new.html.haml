.spacer25
.container
  %h3 Create a new interview
  .row
    .col-xs-12.col-sm-6.form-group
      = semantic_form_for @interview do |f|
        = f.input :candidate,required: false, member_label: Proc.new { |t| h(t.first_name + " " + t.last_name) }, input_html: { class: "form-control" }
        = f.input :position, required: false, input_html: { class: "form-control" }
        %br
        = f.action :submit, :as => :button, button_html: { class: "btn btn-primary" }
