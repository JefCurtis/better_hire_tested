.container
  %h3 Available Candidates
  .row
    .col-xs-12.col-sm-6.form-group
      = semantic_form_for @position do |f|
        = f.input :candidates, as: :check_boxes, member_label: Proc.new { |t| h(t.first_name + " " + t.last_name) }
        = f.action :submit, as: :button, button_html: { class: "btn btn-primary"}



